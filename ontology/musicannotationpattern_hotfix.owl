<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.org/andreapoltronieri/music-annotation-pattern/"
     xml:base="http://purl.org/andreapoltronieri/music-annotation-pattern/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ex="https://example.org/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/">
        <owl:versionIRI rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/0.1/"/>
        <dc:creator xml:lang="en">Andrea Poltronieri</dc:creator>
        <dc:creator xml:lang="en">Jacopo de Berardinis</dc:creator>
        <dc:creator xml:lang="en">Valentina Presutti</dc:creator>
        <dc:license rdf:resource="https://creativecommons.org/licenses/by-sa/4.0/"/>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by-sa/4.0/"/>
        <rdfs:comment xml:lang="en">An Ontology Design Pattern (ODP) for modelling differnt types of musical annotation</rdfs:comment>
        <rdfs:label xml:lang="en">Music Annotation Pattern</rdfs:label>
        <owl:versionInfo xml:lang="en">1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/license"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasAnnotator -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasAnnotator">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator"/>
        <rdfs:comment xml:lang="en">Links a annotation to the Annotator the annotated the file .</rdfs:comment>
        <rdfs:label xml:lang="en">Has Annotator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasAnnotatorType -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasAnnotatorType">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/AnnotatorType"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/includesMusicObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicObservationOf"/>
        <rdfs:comment xml:lang="en">Links the Annotator class to its Annotator Type, describing how the Annotations were generated.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Annotator Type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicAnnotation -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicAnnotation">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicAnnotationOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicalObject"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/includesMusicObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicObservationOf"/>
        <rdfs:comment xml:lang="en">Relates a Score or a Recording to the annotation describing the track.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Annotation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicTimeDuration -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicTimeDuration">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicTimeDurationOf"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeInterval"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeDuration"/>
        <rdfs:label xml:lang="en">Has Music Time Duration</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicTimeIndex -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicTimeIndex">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicTimeIndexOf"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeInterval"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeIndex"/>
        <rdfs:label xml:lang="en">Has Music Time Index</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicTimeIndexComponent -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicTimeIndexComponent">
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeIndex"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeIndexComponent"/>
        <rdfs:label xml:lang="en">Has Music Time Index Component</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicTimeInterval -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicTimeInterval">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicTimeIntervalOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicAnnotation"/>
                    <rdf:Description rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicObservation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeInterval"/>
        <rdfs:label xml:lang="en">Has Music Time Interval</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicTimeValueType -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasMusicTimeValueType">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeDuration"/>
                    <rdf:Description rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeIndexComponent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeValueType"/>
        <rdfs:label xml:lang="en">Has Music Time Value Type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasValue -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValue">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isValueOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicObservation"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicObservationValue"/>
        <rdfs:comment xml:lang="en">It links an Observation to its ObservationValue.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/includesMusicObservation -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/includesMusicObservation">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicObservationOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicAnnotation"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicObservationOf"/>
        <rdfs:comment xml:lang="en">Links a Annotation to the Onservation(s) it contains.</rdfs:comment>
        <rdfs:label xml:lang="en">Includes Observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf">
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator"/>
        <rdfs:comment xml:lang="en">Inverse of hasAnnotator. Links the Annotator to the Annotation annotated by such annotator.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Annotator Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf">
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/AnnotatorType"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicObservationOf"/>
        <rdfs:comment xml:lang="en">Inverse of hasAnnotatorType. Links the Annotator Type to the Annotator class.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Annotator Type Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicAnnotationOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicAnnotationOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicAnnotation"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicalObject"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicObservationOf"/>
        <rdfs:comment xml:lang="en">Inverse of hasAnnotation. It links the Annotation to either a Recording o a Score.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Annotation Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicObservationOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicObservationOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicObservation"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicAnnotation"/>
        <rdfs:comment xml:lang="en">Inverse of includesObservation. Links a  Observation to the Annotation to which it belongs.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Observation Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicTimeDurationOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicTimeDurationOf">
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeDuration"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeInterval"/>
        <rdfs:label xml:lang="en">Is Music Time Duration Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicTimeIndexComponentOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicTimeIndexComponentOf">
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeIndexComponent"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeIndex"/>
        <rdfs:label xml:lang="en">Is Music Time Index Component Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicTimeIndexOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicTimeIndexOf">
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeIndex"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeInterval"/>
        <rdfs:label xml:lang="en">Is Music Time Index Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicTimeIntervalOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isMusicTimeIntervalOf">
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeInterval"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicAnnotation"/>
                    <rdf:Description rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicObservation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="en">Is Music Time Interval Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isValueOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isValueOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicObservationValue"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicObservation"/>
        <rdfs:comment xml:lang="en">Inverse of hasValue. It relates an ObservationValue to the belonging Observation.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Value Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasConfidence -->

    <owl:DatatypeProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasConfidence">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicObservation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasName"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasDurationValue -->

    <owl:DatatypeProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasDurationValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeDuration"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="en">Has Duration Value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasName -->

    <owl:DatatypeProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasName">
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The name of an annotator, which can be either human, artificial (e.g. the name of a software) or describing a group of people (e.g. the name of a crowdsourcing service/event).</rdfs:comment>
        <rdfs:label xml:lang="en">Has Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasTimeValue -->

    <owl:DatatypeProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasTimeValue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeIndexComponent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="en">Has Time Value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator -->

    <owl:Class rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasAnnotatorType"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/AnnotatorType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasName"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/AnnotatorType"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicAnnotation"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicObservation"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeDuration"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeIndex"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeIndexComponent"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeInterval"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeValueType"/>
        <rdfs:comment xml:lang="en">The annotator of a annotation, whether understood as a physical person or as a machine that has performed an automatic transcription.</rdfs:comment>
        <rdfs:label xml:lang="en">Annotator</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/AnnotatorType -->

    <owl:Class rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/AnnotatorType">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicAnnotation"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicObservation"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeDuration"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeIndex"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeIndexComponent"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeInterval"/>
        <owl:disjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicTimeValueType"/>
        <rdfs:comment xml:lang="en">The type of annotator of an annotation, whether human or the result of an automatic or crowdsourced annotation.</rdfs:comment>
        <rdfs:label xml:lang="en">Annotator Type</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/AudioMusicAnnotation -->

    <owl:Class rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/AudioMusicAnnotation">
        <rdfs:subClassOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MusicAnnotation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
