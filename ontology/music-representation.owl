    <!-- https://w3id.org/polifonia/ontology/music-representation/describesFragment -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-representation/describesFragment">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/describes"/>
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-representation/isDescribedByAnnotation"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-representation/Annotation"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-representation/Fragment"/>
        <rdfs:comment xml:lang="en">The relation between an annotation and the fragment it is describing.</rdfs:comment>
        <rdfs:label xml:lang="en">describes fragment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-representation/hasAnnotation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-representation/hasAnnotation">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/describes"/>
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-representation/isAnnotationOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-representation/Analysis"/>
                    <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/music-representation/MusicContent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-representation/Annotation"/>
        <rdfs:comment xml:lang="en">A direct relationship between a music content and an annotation. Some specific content (e.g. elements of a score) might be annotated without the need of an analysis.</rdfs:comment>
        <rdfs:label xml:lang="en">has annotation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-representation/hasAnnotationType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/music-representation/hasAnnotationType">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/isClassifiedBy"/>
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/music-representation/isAnnotationTypeOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-representation/Annotation"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-representation/AnnotationType"/>
        <rdfs:comment xml:lang="en">The relationship that defines the type of an annotation.</rdfs:comment>
        <rdfs:label xml:lang="en">has annotation type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/music-representation/hasAnnotator -->

