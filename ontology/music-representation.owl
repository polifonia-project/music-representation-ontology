        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/music-representation/Annotation"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/music-representation/Annotator"/>
        <rdfs:comment xml:lang="en">The relationship that identifies an agent that contributed to an annotation.</rdfs:comment>
