    <!-- https://purl.org/andreapoltronieri/musicalobject/Envelope -->

    <owl:Class rdf:about="https://purl.org/andreapoltronieri/musicalobject/Envelope">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.org/andreapoltronieri/musicalobject/isEnvelopeOf"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://purl.org/andreapoltronieri/musicalobject/MusicalObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.org/andreapoltronieri/musicalobject/hasAttack"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.org/andreapoltronieri/musicalobject/hasDecay"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.org/andreapoltronieri/musicalobject/hasRelease"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.org/andreapoltronieri/musicalobject/hasSustain"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/musicalobject/Frequency"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/musicalobject/MusicalObject"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/musicalobject/MusicalObjectDuration"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/musicalobject/SoundIntensity"/>
        <rdfs:comment xml:lang="en">The envelope of a musical object&apos;s soundwave. In physics and engineering, the envelope of an oscillating signal is a smooth curve outlining its extremes.</rdfs:comment>
        <rdfs:label xml:lang="en">Envelope</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/andreapoltronieri/musicalobject/Frequency -->

    <owl:Class rdf:about="https://purl.org/andreapoltronieri/musicalobject/Frequency">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.org/andreapoltronieri/musicalobject/isFrequencyOf"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://purl.org/andreapoltronieri/musicalobject/MusicalObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purl.org/andreapoltronieri/musicalobject/hasFrequencyMagnitude"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
            </owl:Restriction>
