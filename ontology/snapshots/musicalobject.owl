        <rdfs:range rdf:resource="https://purl.org/andreapoltronieri/musicalobject/MusicalObject"/>
        <rdfs:comment xml:lang="en">Inverse of hasFrequency. Connects the frequency of a musical event to the musical event itself.</rdfs:comment>
        <rdfs:label xml:lang="en">is frequency of</rdfs:label>
    </owl:ObjectProperty>
    


